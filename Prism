   Name = "Chat Bypasser",
   Callback = function()
   loadstring(game:HttpGet("https://raw.githubusercontent.com/randomizedcomponent/UC/refs/heads/main/3307468c285f4fe535fb5d87b4b053e6.lua"))()
   end,
})

local Button = Tab:CreateButton({
   Name = "Shaders",
   Callback = function()
   local StarterGui = game:GetService("StarterGui")

function callback(Text)
if Text == "Blur" then
loadstring(game:HttpGet("https://pastebin.com/raw/42PPvjWc"))()
elseif Text == "No Blur" then
loadstring(game:HttpGet("https://pastebin.com/raw/vd4cUiTC"))()
end
end

local bn = Instance.new("BindableFunction")
bn.OnInvoke = callback

-- Exibir a notificação
StarterGui:SetCore("SendNotification", {
    Title = "Choice",
    Text = "Choose an option:", -- Adicionei um texto para a notificação ficar mais clara
    Button1 = "Blur",
    Button2 = "No Blur",
    Duration = 5, -- Duração em segundos antes da notificação desaparecer se não houver interação
    Callback = bn
})
   end,
})

local Button = Tab:CreateButton({
   Name = "Copy Pos",
   Callback = function()
   setclipboard(game.Players.LocalPlayer.Character.HumanoidRootPart.Position.X .. ", " .. game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Y .. ", " .. game.Players.LocalPlayer.Character.HumanoidRootPart.Position.Z)
   end,
})

